
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <script src="~/wwwroot/jquery-3.2.1.min.js"></script>
    <script>
        $(function () {
            $("#btnSubmit").click(function () {
                $.ajax({
                    url: "http://localhost:50887/http/GetPersonelDataList",
                    type: "GET",
                    crossDomain: true,
                    dataType: "json",
                    success: function (result) {
                        alert(JSON.stringify(result));
                    },
                    error: function (xhr, status, error) {
                        alert(status + " - " + error);
                    },
                });
            });
        });
    </script>
</head>
<body>
    <div>
        <button id="btnSubmit">Get Personel List</button>

    </div>
</body>
</html>
